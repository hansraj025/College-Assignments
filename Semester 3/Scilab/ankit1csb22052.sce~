clc;
clear;
disp("Enter three numbers to compare.");
A=input("Enter the first number.");
B=input("Enter the second number.");
C=input("Enter the third number.");
if (A==B==C ) then
    disp("All are same");
elseif (A>B)then 
    if(A>C) then 
        disp("maximum:"+string(A));
    else
        disp("maximum:"+string(C));
    end
else
    if (C>B)
        disp("maximum:"+string(C));
    else
        disp("maximum:"+string(B));
        end
end
